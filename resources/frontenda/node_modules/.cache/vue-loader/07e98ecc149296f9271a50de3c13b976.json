["<template >\n<!--    <v-app >-->\n\n         <div class=\"LoadFilePageCard\" :class=\"{ hiddenCls: isFinded }\">\n\n<!--             <div class=\"darknesPlane\">-->\n<!--             </div>-->\n<!--&lt;!&ndash;        <h1>BBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBbb</h1>&ndash;&gt;-->\n\n             <!--                    v-show=\"isFinded == false\" -->\n<!--             :class=\"'myCard'+index\"-->\n<!--             && isFinded == false-->\n                  <v-card\n                      :disabled=\"isEditableNowHere\"\n                      v-if=\"isDisabledCardIndex!=index \"\n\n                      color=\"#333333\"\n                      dark\n                  >\n                      <v-row dense>\n                          <v-col cols=\"11\">\n                              {{index}}\n<!--                              {{isFinded}}-->\n                              <v-card-title v-html=\"`name: ` + currFN\" class=\"headline\"> </v-card-title>\n                              <v-card-subtitle> TLF: {{currTEL}}</v-card-subtitle>\n                              <v-card-subtitle>EMAIL: {{currEMAIL}}</v-card-subtitle>\n\n                          </v-col>\n                          <v-col cols=\"1\">\n\n                                  <v-btn :id='\"btnEdit\"+index' @click=\"btnDel(index)\">\n                                      <v-icon>{{ icons.mdiDelete }}</v-icon>\n                                  </v-btn>\n\n\n                                  <v-btn id=\"btnDel\" @click=\"edit(index)\">\n                                      <v-icon>{{ icons.mdiPencil }}</v-icon>\n                                  </v-btn>\n                          </v-col>\n\n                      </v-row>\n                  </v-card>\n<!--                 <div v-if=\"isEditCard==true\">-->\n                 <div v-if=\"isDisabledCardIndex==index\">\n                     <router-view @saveEditData=\"saveEditData\" :fileData=\"fileData\" :index=\"index\"></router-view>\n                 </div>\n\n<!--             <div id=\"temp1\"></div>-->\n<!--             {{findingTextHere}}-->\n      </div>\n\n\n<!--    </v-app>-->\n</template>\n\n<script>\n\n\n    import {\n        mdiPencil,\n        mdiDelete,\n    } from '@mdi/js'\n\n\nexport default {\n  name: 'LoadFilePageCard',\n\n    props: ['fileData', 'index', 'isDisabledCardIndex', 'isEditableNow', 'findingText', 'isFindedCard','setHideCard'],   //\n\n    data() {\n        return {\n\n            // currFN:  null,//this.fileData.FN,\n            // currTEL: null,//this.fileData.TEL,\n            // currEMAIL: null,//this.fileData.EMAIL,\n\n            //currFN:  this.fileData.FN,\n            //currTEL: this.fileData.TEL,\n            //currEMAIL: this.fileData.EMAIL,\n\n            isFinded: false,\n\n            icons: {\n                mdiPencil,\n                mdiDelete,\n            },\n        }\n    },\n\n    methods: {\n\n\n      // пердача индекса, который нужно удалить\n        btnDel(index){\n            //console.log(index)\n            this.$emit('btnDel', index);\n        },\n\n      // переход на страницу редактирование данных\n        edit(i){\n            //console.log(i);\n            this.$emit('isEdit', i)\n            this.$router.push({name: 'Edit' , params: {id: i}})\n        },\n\n      // возврат со страницы редактирование данных\n        saveEditData(i,FN,TEL,EMAIL){\n            this.$emit('EditData', i, FN, TEL, EMAIL);\n        },\n\n        setHide(b){\nb\n            this.isFinded = b;\n        }\n\n    }, // methods:\n\n    computed:{\n        // если кликнули не по этому компоненту он disabled, вначале enabled все\n        isEditableNowHere: function () {\n            if(this.isDisabledCardIndex == null){\n                    //this.isEditableNow = false;\n                return false;\n            }\n            else if(this.isDisabledCardIndex == this.index){\n                //this.isEditableNow = false;\n                return false;\n            }\n            return true;\n        },\n\n        // isFinded: function(){\n        //     //return this.isFindedCard\n        //     return false\n        // },\n\n        // isFinded: function() {\n        //     //console.log(this.isFinded)\n        //     return \" \";\n        // },\n\n        currFN: function() {\n    //         //   let str = this.fileData.FN.toLowerCase();\n    //         //   let myFind = this.findingTextHere.toLowerCase().trim();\n    //         //   let findedIndex = str.search(myFind)\n    //         // findedIndex\n    //         //this.$emit('setHideCard', false)\n    // // console.log(\"   str: \" + myFind)\n    // // if(myFind == 1)\n    // // this.setHide(false);\n    // // if(myFind == 2)\n    // // this.setHide(true);\n    // //return findedIndex\n    //         //this.setHide(false);\n    //\n    //         //    if(myFind && myFind.length > 0) {\n    //         //         if (findedIndex != -1) {\n    //         //             //this.$emit('setHide', true)\n    //         //             this.setHide(false);\n    //         //\n    //         //\n    //         //             // console.log(\"   str: \" + str)\n    //         //             //console.log('hhhhh: ' + this.index + \"   num: \" + findedIndex)\n    //         //             //console.log('hhhhh: '+myFind.length)\n    //         //             let resultStr = this.fileData.FN.slice(0, findedIndex)\n    //         //                 + \"<mark>\" +\n    //         //                 this.fileData.FN.slice(findedIndex, findedIndex + myFind.length)\n    //         //                 + \"</mark>\" +\n    //         //                 this.fileData.FN.slice(findedIndex + myFind.length);\n    //         //             return resultStr;\n    //         //         }\n    //         //\n    //         //        this.setHide(true);\n    //         //        //this.$emit('setHide', true)\n    //         //        return this.fileData.FN;\n    //         // }\n    //         // //console.log('yyyyyyyyyy: '+\"myFind.length\")\n    //         // this.setHide(false);\n    //         // //this.$emit('setHide', false)\n            return this.fileData.FN;\n\n        },\n        currTEL: function(){\n            return this.fileData.TEL;\n        },\n        currEMAIL: function(){\n            return this.fileData.EMAIL;\n        },\n\n        findingTextHere:  function(){\n            //console.log(this.findingTextHere)\n            return this.findingText;\n        },\n\n\n    }, // compute:\n\n\n     watch: {\n         findingTextHere: function(){\n\n              let str = this.fileData.FN.toLowerCase();\n              let myFind = this.findingText.toLowerCase().trim();\n              let findedIndex = str.search(myFind)\n\n              if(myFind && myFind.length > 0) {\n                  if (findedIndex != -1) {\n                      //this.setHide(false);\n                      this.$emit('setHideCard', this.index, 'show')\n\n                           //console.log('hhhhh: '+myFind.length)\n                          let resultStr = this.fileData.FN.slice(0, findedIndex)\n                              + \"<mark>\" +\n                              this.fileData.FN.slice(findedIndex, findedIndex + myFind.length)\n                              + \"</mark>\" +\n                              this.fileData.FN.slice(findedIndex + myFind.length);\n                           this.currFN = resultStr;\n                          return ;\n                      }\n\n                     //this.setHide(true);\n                     this.$emit('setHideCard', this.index, 'hide')\n                     this.currFN = this.fileData.FN;\n                     return ;\n              }\n              //this.setHide(false);\n             this.$emit('setHideCard', this.index, 'show')\n             this.currFN = this.fileData.FN;\n             return ;\n         }\n     },\n\n    beforeMount: function() {\n        console.log(\"--------beforeMount111--