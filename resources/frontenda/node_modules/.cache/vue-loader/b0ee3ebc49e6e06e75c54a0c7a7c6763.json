;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;;AAGA;AACA;AACA;AACA;AACA;AACA;;;;;AAKA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA","file":"LoadFilePage.vue","sourceRoot":"src/components","sourcesContent":["<template>\n    <v-app>\n      <div class=\"hello\">\n<!--        <h1>BBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBbb</h1>-->\n\n<!--          // загрузка файла-->\n          <v-file-input\n              accept=\".-vcf-\"\n              label=\"Click here to select a .txt file\"\n              outlined\n              v-model=\"chosenFile\"\n          >\n          </v-file-input>\n\n<!--          // кноки 'читать файл',  'сохранить'-->\n          <v-row >\n              <v-col cols=\"2\">\n                <v-btn right @click=\"importTxt\">Read File</v-btn>\n              </v-col>\n              <v-col cols=\"2\">\n                <v-btn right @click=\"saveToServe\">Save File</v-btn>\n              </v-col>\n\n          </v-row >\n\n\n              <v-card-text v-if=\"data=='No File Chosen'\"><p>{{data}}</p></v-card-text>\n\n<!--                  <v-card-text v-for=\"(item, index) in mString\" :key=\"index\"><p>{{index}}: {{mString[index]}}</p></v-card-text>-->\n\n          <div v-if=\"data!='No File Chosen'\">\n              <v-container class=\"aa11\" v-for=\"(item, index) in mString\" :key=\"index\">\n\n<!--                  //  V-APP КОМПОНЕНТА   --  СДЕЛАННО НЕКОРPЕКТНО-->\n                  <div v-if=\"isEditCard!=this.$route.params\">\n                    <LoadFilePageCard :fileData=\"item\"\n                                    :index=\"index\"\n                                    @isEdit=\"isEdit\"\n                                    @btnDel=\"btnDel\"\n                                    @EditData=\"EditData\"\n                    ></LoadFilePageCard>\n                  </div>\n              </v-container>\n\n          </div>\n\n\n      </div>\n    </v-app>\n</template>\n\n<script>\n\nimport axios from \"axios\";\n\nimport LoadFilePageCard from './LoadFilePageCard';\n//import HelloWorld from \"./HelloWorld\";\n\nexport default {\n  name: 'HelloWorld',\n\n\n    components: {\n        LoadFilePageCard,\n    },\n\n    data() {\n        return {\n            chosenFile: null,\n            data: null,\n            mString: [\n                // {\n                //     FN: 'xx2',\n                //     TEL: '+00000000002',\n                //     EMAIL: 'xxx@xx2.com',\n                // }\n            ],\n            mStringEl: {\n                FN: 'xx2',\n                TEL: '+00000000002',\n                EMAIL: 'xxx@xx2.com',\n            },\n            isEditCard: null,  //  номер карты которая редактируется // либо режим редактирования карты, либо обычный\n        }\n    },\n\n    methods: {\n\n\n      // удаление из массива из дочернего компонента\n        btnDel(i){\n            this.mString.splice(i, 1)\n            //console.log(i);\n            //console.log('this.mString1111111111'+index)\n        },\n\n        // режим редактирования карты\n        isEdit(i){\n            this.isEditCard = i;\n        },\n      // замена значения в массиве из дочернего компонента\n        EditData(i,FN,TEL,EMAIL){\n            this.isEditCard = null;\n            this.mString[i].FN = FN;\n            this.mString[i].TEL = TEL;\n            this.mString[i].EMAIL = EMAIL;\n        } ,\n\n        // сохранение на сервер\n        saveToServe(){\n            console.log(\"--------save--------\");\n            //console.log( this.mString[0].FN);\n\n            axios.post(\"http://laravel-vue-cli-vuetify/api/saveInformList\" , {fileData: this.mString})\n                .then(res => {\n                    console.log(res.data.status)\n                    //console.log(res.data.fileData)\n\n                });\n        },\n\n\n\n        // чтение из файла\n        importTxt() {\n\n            if (!this.chosenFile) {\n                this.data = \"No File Chosen\"\n            }else {\n                var reader = new FileReader();\n                reader.readAsText(this.chosenFile);\n                reader.onload = () => {\n                    this.data = reader.result;\n\n\n                    //   обработка файла\n                    let mStringTime = this.data.split('\\n');      //  Cut  Text\n\n                    for (let i = 0; i <= mStringTime.length - 1; i++) {\n\n                        //this.sss = mStringTime;\n\n                        // 111111111111111111111111111111111111111111\n                        let str = mStringTime[i].indexOf('BEGIN:VCARD');\n                        if (str != -1) {\n                            this.mStringEl.FN = 'none';\n                            this.mStringEl.TEL = 'none';\n                            this.mStringEl.EMAIL = 'none';\n                        }\n\n                        // 22222222222222222222222222222222222222222\n                        let strNum = mStringTime[i].indexOf('FN:');\n                        if (strNum != -1) {\n                            let strOrigin = mStringTime[i].trim();\n                            strNum = strOrigin.indexOf(':');\n                            let strResult = strOrigin.slice(strNum + 1);\n                            if(!strResult==' '){\n                                this.mStringEl['FN'] = strResult;\n                            }\n                            // console.log(str+\"  ********* \"+ strResult)\n                        }\n\n                        // 33333333333333333333333333333333333333333\n                        strNum = mStringTime[i].indexOf('TEL;');\n                        if (strNum != -1) {\n                            let strOrigin = mStringTime[i].trim();\n                            strNum = strOrigin.indexOf(':');\n                            let strResult = strOrigin.slice(strNum + 1);\n                            if(!strResult==' ') {\n                                this.mStringEl['TEL'] = strResult;\n                            }\n                            //console.log(str+\"  ********* \"+ strResult)\n                        }\n\n                        // 4444444444444444444444444444444444444444\n                        strNum = mStringTime[i].indexOf('EMAIL');\n                        if (strNum != -1) {\n                            let strOrigin = mStringTime[i].trim();\n                            strNum = strOrigin.indexOf(':');\n                            let strResult = strOrigin.slice(strNum + 1);\n                            if(!strResult==' ') {\n                                this.mStringEl['EMAIL'] = strResult;\n                            }\n                            //console.log(str+\"  ********* \"+ strResult)\n                        }\n\n                        // 5555555555555555555555555555555555555555\n                        strNum = mStringTime[i].indexOf('END:VCARD');\n                        if (strNum != -1) {\n\n                            this.mString.push({\n                                FN: this.mStringEl.FN,\n                                TEL: this.mStringEl.TEL,\n                                EMAIL: this.mStringEl.EMAIL\n                            });\n                            //this.mString.push(this.mStringEl);\n                            //console.log(\"  ********* \"+ this.mString.length + '--:--'+this.mString[this.mString.length-1].FN)\n                            //console.log(this.mString)\n                        }\n                    }\n                    // console.log(\"uuuuuuuu\")\n\n                    //--------------------------------\n\n\n                    // for(let i=0; i<=this.mString.length-1; i++) {\n                    //     console.log(\"  ********* \"+ i + '--:--'+this.mString[i].FN)\n                    // }\n                    //console.log(this.mString)\n                }\n            }\n\n\n\n\n        } // importTxt()\n    }, // methods:\n\n\n    mounted: function() {\n        console.log(\"--------mounted--------\");\n        axios.post(\"http://laravel-vue-cli-vuetify/api/1\" , {})\n            .then(res => {\n                console.log(res.data.status)\n                console.log(res.data.file)\n            });\n    },\n\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n</style>\n"]}]}